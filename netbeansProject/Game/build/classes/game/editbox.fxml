<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="350.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EditboxController">
    <stylesheets>
        <URL value="@game.css" />
    </stylesheets>
   <children>
      <TextArea fx:id="editArea" prefHeight="150.0" prefWidth="351.0" />
      <Button fx:id="okButton" layoutX="232.0" layoutY="161.0" mnemonicParsing="false" styleClass="register_btn" text="OK" />
      <Button fx:id="cancelButton" layoutX="276.0" layoutY="161.0" mnemonicParsing="false" onAction="#closeEditBox" styleClass="register_btn" text="Cancel" />
   </children>
</AnchorPane>
